
  # pysh PYSH

">>> pipe multiline"
 a = [1,2,3] | gen
 a |
   map@ _ +1 | 
   map@_*3

">>> case "
def match_case(x,y)
1,"a" => {print("case1", x, y), 1}
2, _ => y
_, [1,2,[]] => {print("case list"), 3}
True, ("c", 0.01, [x0,x1,xs] ) => xs
otherwise => print("otherwise")
end

match_case(1, "a") == 1
match_case(2, True)
match_case(None, [1,2]) == 3
match_case(True, ["c", 0.01, [1,2,3,4,5,6]]) == [3,4,5,6]
match_case("d", [1])
