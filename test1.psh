def qsort(lst)
    if (len(lst) <= 1)
        return lst
    end
    leftpart = filter(lst, _ < lst[0]) | list
    rightpart = filter(lst, _ > lst[0]) | list
    eqpart = filter(lst, L(x): x == lst[0]) | list
    return  qsort(leftpart) + eqpart + qsort(rightpart)
end

